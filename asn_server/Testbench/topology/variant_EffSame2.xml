<?xml version="1.0" ?>
	<network>
		<node pi_id="3">
			<algorithm path="Testbench/system/readfile_module" executable="./module_readfile.py">
                <output target_pi_id="3" pipe_id="1"></output>
                <output target_pi_id="3" pipe_id="2"></output>
				<parameter param="-f AudioData/Audio_2Chan_80ppm.wav"></parameter>
			</algorithm>

	    <algorithm path="Testbench/system/processing_modules" executable="./module_process_resampling.py">
                <input source_pi_id="3" pipe_id="1"></input>
		<input source_pi_id="3" pipe_id="3"></input>
                <output target_pi_id="3" pipe_id="6"></output>
		<parameter param="-l /home/pi/asn_daemon/logfiles/sro_info.log"></parameter>
		<parameter param="--SamplingRateOffset 0 --WindowSize 20 --Name Resampling CH1"></parameter>
            </algorithm>

	    <algorithm path="Testbench/system/processing_modules" executable="./module_process_resampling.py">
                <input source_pi_id="3" pipe_id="2"></input>
		<input source_pi_id="3" pipe_id="4"></input>
                <output target_pi_id="3" pipe_id="5"></output>
		<parameter param="-l /home/pi/asn_daemon/logfiles/sro_info.log"></parameter>
		<parameter param="--SamplingRateOffset 0 --WindowSize 20 --Name Resampling CH2"></parameter>
            </algorithm>


            <algorithm path="Testbench/system/processing_modules" executable="./module_process_all_efficient.py">
                <input source_pi_id="3" pipe_id="5"></input>
                <input source_pi_id="3" pipe_id="6"></input>
                <output target_pi_id="3" pipe_id="3"></output>
                <output target_pi_id="3" pipe_id="4"></output>
				<parameter param="-l /home/pi/asn_daemon/logfiles/results.log"></parameter>
            </algorithm>
        </node>
	</network>
